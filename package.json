    "next": "14.2.35",
    "next-auth": "^5.0.0-beta.30",
    "react": "^18",
    "react-dom": "^18",
    "react-email": "^5.2.5",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.71.1",
    "resend": "^6.8.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
