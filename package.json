    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8",
    "prettier": "^3.8.1",
    "prisma": "5.12.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
